{
  "manifest_version": 3,
  "name": "Timer",
  "description": "This extension will be a productivity tool",
  "version": "1.0",
  "action": { "default_icon": "icon.png", "default_popup": "window.html" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ],
  "permissions": ["contextMenus", "tabs"]
}
